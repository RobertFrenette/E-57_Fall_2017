<mapping>
    <class name="edu.hes.e57.demo.domain.Autos">
        <field name="autos" type="edu.hes.e57.demo.domain.Auto" collection="arraylist">
            <bind-xml name="auto"/>
        </field>
    </class>

    <class name="edu.hes.e57.demo.domain.Auto" identity="id">
        <map-to xml="auto" />

        <field name="id" type="long">
            <bind-xml name="id" node="element"/>
        </field>
        <field name="model" type="string">
            <bind-xml name="model" node="element" />
        </field>
        <field name="year" type="string">
            <bind-xml name="year" node="element" />
        </field>
        <field name="msrp" type="string">
            <bind-xml name="msrp" node="element" />
        </field>
        <field name="version" type="integer">
            <bind-xml name="version" node="element" />
        </field>
    </class>
</mapping>
